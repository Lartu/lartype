#Should check if wkhtmltopdf is installed
echo "[*] Starting Lartype server"
python /usr/local/bin/_lartype/threadingserver.py &
SERVER=$!
echo "[*] Started with PID $SERVER"
/usr/local/bin/_lartype/wkhtmltopdf -B 30mm -T 30mm -R 20mm -L 20mm --disable-smart-shrinking --header-html http://localhost:23416/lartype/header.html --footer-html http://localhost:23416/lartype/footer.html http://localhost:23416/lartype/lartype.html output.pdf 
kill $SERVER
echo "[*] Done!"
